#计算累计盈亏
select sum(profit)as sum_profit from tb_future_fill_close_detail where user_id=
#最大盈利
select max(profit)as max_profit from tb_future_fill_close_detail where user_id=
#累计总盈利
select sum(profit)as sum_profit_profit from tb_future_fill_close_detail where  profit_tag=1 and user_id=
#累计总亏损
select sum(profit)as sum_profit_loss from tb_future_fill_close_detail where  profit_tag=0 and user_id=
#累计总盈利手数
select sum(fill_size)as sum_fill_size_profit from tb_future_fill_close_detail where  profit_tag=1 and user_id=
#累计总亏损手数
select sum(fill_size)as sum_fill_size_loss from tb_future_fill_close_detail where profit_tag=0 and user_id=
#总手续费
select sum(open_fee+close_fee)as sum_fee from tb_future_fill_close_detail where user_id=
#高频次数
select count(*) as sum_gaopin from tb_future_fill_close_detail where position_time<=600 and profit_tag=1 and user_id=
#持仓时间
select sum(position_time)as sum_position_time from tb_future_fill_close_detail where user_id=
#累计交易次数
select count(*)as sum_count from tb_future_fill_close_detail where user_id=
#潜在盈利持仓时间
select sum(position_time_profit*fill_size)as sum_position_time_profit from tb_future_fill_close_detail where user_id=
#潜在亏损持仓时间
select sum(position_time_loss*fill_size)as sum_position_time_loss from tb_future_fill_close_detail where user_id=
#潜在盈利
select sum(potential_profit)as sum_potential_profit from tb_future_fill_close_detail where user_id=
#潜在亏损
select sum(potential_loss)as sum_potential_loss from tb_future_fill_close_detail where user_id=
#######################################
